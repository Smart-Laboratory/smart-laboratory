<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <style>
        *{
            font-family:SimSun;
            font-size: 9pt;
        }
        html * {
            margin: 0 auto;
        }

        body {
            width: 100%;
            /*height: 13.5cm;*/
        }

        table {
            width: 100%;
        }

        .content {
            /*width: 100%;*/
            margin: 10px 30px 10px 30px;
        }

        .head {
            width: 100%;
            padding-bottom:2px;
            font-size: 10pt !important;
        }

        .patientinfo {
            width: 100%;
            border-top: 2px solid #000;
            border-bottom: 2px solid #000;
            padding-bottom:4px;
        }

        .patientinfo td {
            /*padding-top:2px;
            padding-bottom:2px;*/
        }

        .patientinfo td span {
            padding-left: 5px;
        }

        .result  {
            width: 100%;
            height: 8.5cm;
        }

        .resultL{
            width: 396px;
            border-right: 1px solid #000;
            float: left;
        }
        .resultR{
            width: 397px;
            float: right;
        }

        .result table {
            width: 100%;
        }

        .result table tr th {
            text-align: left;
            font-bold: weight;
            border-bottom: 1px solid #000;
            padding-bottom:2px;
        }
        .resultList{
            table-layout: fixed;
        }
        .resultList td{
            text-align: left;
            #if($!results.size() <10)
                font-size: 10.5pt;
            #end
            height: 15px;
            line-height: 15px;
            overflow:hidden;
            white-space:nowrap;
        }
        .footer {
            width: 100%;
        }

        .footer table {
            width: 100%;
        }

        .footer tr td {
            /*font-size: 7.5pt;*/
        }

        .headtitle {
            font-size: 18pt;
            font-weight: bold;
        }

        .llogo img, .rlogo img {
            width: 160px;
            height: 50px;
        }

        .rlogo {
            text-align: right;
        }

        .line1 td {
            border-bottom: 1px solid #000;
            padding-bottom:2px;
        }

        .line2 td {
            border-bottom: 2px solid #000;
            padding-bottom:2px;
        }
    </style>
</head>
<body>
<div class="content">
    #parse ("/template/testReportHead.vm")
    <!--结果信息 start-->
    <div class="result">
        <table cellspacing="0" cellpadding="0">
            <colgroup>
                <col width="50%"/>
                <col width="50%"/>
            </colgroup>
            <tr>
                <td style="vertical-align:top;border-right:1px solid #000">
                    <table cellpadding="0" cellspacing="0" class="resultList">
                        <colgroup>
                            <col width="40%"/>
                            <col width="15%"/>
                            <col width="5%"/>
                            <col width="15%"/>
                            <col width="25%"/>
                        </colgroup>
                        <tr>
                            <th>检验项目</th>
                            <th>结果</th>
                            <th>&#160;</th>
                            <th>单位</th>
                            <th>参考值</th>
                        </tr>
                        #set($listSize1=$!results.size()%2)
                        #set($listSize2=$!results.size()/2)

                        #foreach($item in $results)
                            #if($velocityCount<=($!listSize2 +$!listSize1))
                                <tr>
                                    <td >$!item.testName</td>
                                    <td>$!item.testResult</td>
                                    <td>$!item.resultFlag</td>
                                    <td>$!item.unit</td>
                                    <td>$!item.reference</td>

                                </tr>
                            #end
                        #end
                    </table>
                </td>
                <td style="vertical-align:top">
                    <table cellpadding="0" cellspacing="0" class="resultList">
                        <colgroup>
                            <col width="40%"/>
                            <col width="15%"/>
                            <col width="5%"/>
                            <col width="15%"/>
                            <col width="25%"/>
                        </colgroup>
                        <tr>
                            <th style="padding-left: 15px;">检验项目</th>
                            <th>结果</th>
                            <th>&#160;</th>
                            <th>单位</th>
                            <th>参考值</th>
                        </tr>
                        #foreach($item in $results)
                            #if($velocityCount>$listSize2)
                                <tr>
                                    <td style="padding-left: 15px;">$!item.testName</td>
                                    <td>$!item.testResult</td>
                                    <td>$!item.resultFlag</td>
                                    <td>$!item.unit</td>
                                    <td>$!item.reference</td>
                                </tr>
                            #end
                        #end
                    </table>
                </td>
            </tr>
        </table>
    </div>
    <!--结果信息 end-->
    #parse ("/template/testReportFooter.vm")
</div>
</body>
</html>